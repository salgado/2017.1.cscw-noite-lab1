<script>
$(document).ready(function(){
    $('section').hide();
    $('#capitulo1').show();
});
function inserir(capitulo){
    $('section').hide();
    $('#'+capitulo).show();
}

function voltar(){
    $('html, body').animate({
   	 scrollTop: $('#header').offset().top
    }, 2000);
}
</script>


